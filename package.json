{
  "name": "veilarbdetaljerfs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --mode lokal",
    "dev": "vite --mode lokal",
    "build": "tsc && vite build --base=https://cdn.nav.no/obo/prod/veilarbdetaljerfs/dist",
    "build:dev": "tsc && vite build --base=https://cdn.nav.no/obo/dev/veilarbdetaljerfs/dist",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier:check": "prettier --check src --config .prettierrc.json",
    "prettier:write": "prettier --write src --config .prettierrc.json",
    "test": "vitest",
    "build:gh-pages": "tsc && vite build --mode demo --base=/veilarbdetaljerfs/",
    "prepare": "husky"
  },
  "dependencies": {
    "@mswjs/storage": "0.1.0",
    "@navikt/aksel-icons": "7.14.3",
    "@navikt/arbeidssokerregisteret-utils": "1.7.0",
    "@navikt/ds-css": "7.14.3",
    "@navikt/ds-react": "7.14.3",
    "constate": "3.3.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "swr": "2.3.6",
    "vite-plugin-svgr": "4.3.0"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@mswjs/data": "0.16.2",
    "@navikt/aksel-stylelint": "7.14.3",
    "@testing-library/react": "16.3.0",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.1",
    "eslint": "9.33.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.5",
    "msw": "2.10.5",
    "prettier": "3.6.2",
    "stylelint": "16.23.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.3",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "src/**/*.css": [
      "stylelint"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "stylelint": {
    "extends": "@navikt/aksel-stylelint/recommended"
  }
}
