{
  "name": "veilarbdetaljerfs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build --base=https://veilarbdetaljer.intern.nav.no",
    "build:dev": "tsc && vite build --base=https://cdn.nav.no/obo/dev/veilarbdetaljerfs/dist",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier:check": "prettier --check src --config .prettierrc.json",
    "prettier:write": "prettier --write src --config .prettierrc.json",
    "test": "vitest",
    "build:gh-pages": "tsc && vite build --mode development --base=/veilarbdetaljerfs/",
    "prepare": "husky"
  },
  "dependencies": {
    "@mswjs/storage": "0.1.0",
    "@navikt/aksel-icons": "7.2.1",
    "@navikt/arbeidssokerregisteret-utils": "1.0.0",
    "@navikt/ds-css": "7.2.1",
    "@navikt/ds-react": "7.2.1",
    "constate": "3.3.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "swr": "2.2.5",
    "vite-plugin-svgr": "4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@mswjs/data": "0.16.2",
    "@navikt/aksel-stylelint": "7.2.1",
    "@testing-library/react": "16.0.1",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "9.12.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "globals": "15.11.0",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.11",
    "msw": "2.7.0",
    "prettier": "3.4.2",
    "stylelint": "16.12.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.9.0",
    "vite": "5.4.9",
    "vitest": "2.1.3"
  },
  "lint-staged": {
    "src/**/*.css": [
      "stylelint"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "stylelint": {
    "extends": "@navikt/aksel-stylelint/recommended"
  }
}
